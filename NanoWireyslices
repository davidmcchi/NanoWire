(set! sources
	(list

		(make source
			(src
				(make continuous-src
					(wavelength 5.32)
					(start-time 0.0)
					(width 2.0)
				)
			)
			(component Ey)
			(center 0.0 0.0 40.0)
			(size 40.0 50.0 0.0)
		)
	)
)

(set! pml-layers
    (list
        (make pml
        	(direction Z)
            (thickness 10.0)
        )
    )
)
(set! symmetries
        (list
                (make mirror-sym
                        (direction X)
                )
                (make mirror-sym
                        (direction Y)
			(phase -1)
                )
        )
)
(set! k-point
;may need to adjust k point for source periodicity
    (vector3 0.0 0.0 )
)
(run-until 100.0

        (to-appended "DPWR4"
                (at-every 25.0
                        (in-volume
                                (volume
                                        (center 0.0 0.0 36.0)
                                        (size 100.0 300.0 0.0)
                                )
                        output-dpwr
                        )
                )
        )
        (to-appended "DPWR1"
